<html lang="en">
  <head>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.13/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.15/angular-ui-router.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.13/angular-cookies.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="http://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"></script>
    <script src="js/loginApp.js"></script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport"  content="width=device-width, initial-scale=1">
    {#<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3mobile.css">#}
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/loginform.css">
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Muli" />
  </head>

<body>
   <div class="header"></div>
   <div class="border"></div>

  <div class="container" ng-app="myApp">
   <script type="text/ng-template" id="login.html">
    <form ng-submit="formSubmit()" class="form" name="loginform">
        <div class="row" id="rowElement">
            <div class="col-md-4 col-sm-2 col-xs-2"></div>
            <div class="col-md-4  col-sm-8 col-xs-6">
            <div class="form-group">
                <input type="email" class="form-control input-lg" ng-model="emailaddress" placeholder="Email address" required=""/>
                <span ng-show="loginform.emailform.$error.email">Not a valid e-mail address</span>
            </div>

            <div class="form-group">
                <input type="password" class="form-control input-lg" ng-model="password" placeholder="Password" required=""/>
            </div>

            <div class="form-group">
                <button type="submit" class="btn btn-default btn-sm">login</button>
                <span class="text-danger">{{'{{ error}}' }}</span>
            </div>

            <div class="form-group col-md-offset-2 col-sm-offset-3 col-xs-offset-3" >
               <input type="checkbox" class="largerCheckbox" ng-model="rememberme" id="remember_me" name="_remember_me" checked />
               <label for="remember_me">remember me</label>
            </div>

            <div class="form-group col-md-offset-5 col-sm-offset-4 col-xs-offset-7">
                <a class="infolink" href="#"></a>
            </div>

            <div class="form-group col-md-offset-3 col-sm-offset-3 col-xs-offset-3">
                <input type="text" class="resetpwd" id="pwdreset" value="I forgot my password"/>
             </div>

        </div>
        <div class="col-md-4 col-sm-2 col-xs-4"></div>
        </div>
    </form>

</script>

<script type="text/ng-template" id="home.html">
    <div class="row" id="rowElement2">
        <div class="col-sm-1 col-xs-1"></div>
        <div class="col-sm-11 col-xs-11"></div>
           <h3>MY IMAGE LIBRARY</h3>
           {# <div class="white-box">#}
            <div class="img-gallery">
             <a target="_top" href="images/test.jpg">
                <img src="images/test.jpg" class="img-thumbnail" alt="image1" >
             </a>
             <a target="_top" href="images/test.jpg">
                <img src="images/test.jpg" class="img-thumbnail" alt="image2" >
             </a>
             <a target="_top" href="images/test.jpg">
                <img src="images/test.jpg" class="img-thumbnail" alt="image3" >
             </a>
             <a target="_top" href="images/test.jpg">
               <img src="images/test.jpg" class="img-thumbnail" alt="image4" >
             </a>
             <a target="_top" href="images/test.jpg">
               <img src="images/test.jpg" class="img-thumbnail" alt="image5" >
             </a>
             <a target="_top" href="images/test.jpg">
                <img src="images/test.jpg" class="img-thumbnail" alt="image6" >
             </a>
            </div>
            <div class="col-sm-offset-3 col-xs-offset-2">
            <input type="text" class="viewmore" id="viewmore" value="VIEW MORE"/>
            </div>

        <form ng-submit="uploadPhoto()" class="uploadform" name="uploadform">
        <div class="upload-photo-cls">
            <h4>UPLOAD OR<br>TAKE PHOTO</h4>
            <input id="upload-photo" type="file" accept="image/*">
            <label for="upload-photo"></label>
        </div>
        <div>
             <button type="submit" class="upload-btn" value="upload">upload</button>
         </div>
        </form>
    </div>

        {#  <a ui-sref="login">Back</a> #}

</script>

<div ui-view></div>
</div>

</body>


